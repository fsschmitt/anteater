{
  "name": "anteater",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start-p": "node ./puppeteer/blueant.js",
    "start": "electron .",
    "build": "parcel build electron/index.html --public-url .",
    "serve": "parcel electron/index.html",
    "watch": "parcel electron/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-mac": "npm run build && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "npm run build && electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "npm run build && electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/icon.png --prune=true --out=release-builds",
    "package-all": "npm run package-mac && npm run package-win && npm run package-linux"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.6",
    "puppeteer": "^2.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "tailwindcss": "^1.2.0"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "electron": "^8.0.0",
    "electron-is-dev": "^1.1.0",
    "electron-packager": "^14.2.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "parcel": "^1.12.4"
  }
}
